  "description": "Server side code for leon",
  "main": "app.js",
  "scripts": {
    "dev": "prettier --write . && nodemon",
    "dev:debug": "prettier --write . && node --inspect-brk -r ts-node/register -r tsconfig-paths/register app.ts",
    "dev_docker": "nodemon",
    "lint": "prettier --write .",
    "build": "tsc && ef-tspm",
    "clean": "rm -rf ./build",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "typeorm:sync": "npm run typeorm schema:sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leon-OnlineLearning/Leon-Serverside.git"
  },
